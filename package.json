		"style-loader": "^1.2.1",
		"ts-jest": "^26.1.1",
		"ts-loader": "^7.0.5",
		"typescript": "^3.9.6",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^4.2.2"
